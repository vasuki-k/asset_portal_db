define(['ojs/ojcore', 'knockout', 'jquery'],
        function (oj, ko, $)
        {
            function fetchData(url) {
        //        console.log(url);
                return $.getJSON(url);
            }
            function doAjax(url, type, payLoad) {
                if (!type) {
                    type = 'GET';
                }
                $.ajaxSetup({contentType: "application/json; charset=utf-8"});
                return   $.ajax({
                    url: url,
                    type: type,
                    data: payLoad,
                    datatype: "json",
                    processData: false
                    
                });


            }

            return {fetchData: fetchData,
            postData: doAjax};
        });


                